<script type="text/ecmascript">
    $(document).ready(function () {
        var options = {
            weekday: "numeric", year: "numeric", month: "numeric",
            day: "numeric", hour: "numeric", minute: "numeric"
        };
        var date = new Date();
        var Day = date.getDate();
        var mounth = date.getMonth() + 1;
        var Year = date.getFullYear();
        var Time = date.toLocaleTimeString("en-US");
        var TimeLength = date.toLocaleTimeString("en-US").length;
        var FullDate = mounth + '/' + Day + '/' + Year + " " + date.toLocaleTimeString("en-US");
        if (Time.indexOf("GMT") != -1) {
            FullDate = mounth + '/' + Day + '/' + Year + " " + date.toLocaleTimeString("en-US").substring(0, TimeLength - 5);
        }
        $.ajax({
            url: '/User/SetTO',
            type: 'post',
            data: { "_LocalTime": FullDate }
        });
    });
    $(document).ready(function () {

        $('#contact_form').validate({

            rules: {
                name: {
                    required: true,
                    minlength: 5
                },
                email: {
                    required: true
                },
                phone: {
                    required: true

                },
                message: {
                    required: true,
                    minlength: 100
                }
            },
            messages: {
                name: {
                    required: "please enter your Name",
                    minlength: "5 characters as minimum "
                },
                email: {
                    required: "please enter your Email"
                },
                phone: {
                    required: "please enter your Phone"
                },
                message: {
                    required: "please type your text message",
                    minlength: "100 characters as minimum"
                }
            },
            submitHandler: function (form) { // for demo
                $(function () {
                    var name = $('#name').val();
                    var email = $('#email').val();
                    var phone = $('#phone').val();
                    var message = $('#message').val();

                    var data = { '_Name': name, '_Email': email, "_phone": phone, '_Message': message };
                    $.ajax({
                        url: '/Home/SendEmail',
                        type: 'post',
                        data: data,
                        beforeSend: function () {

                            $('#message_send').attr("disabled", "disabled");

                        },
                        complete: function () {

                            $('#message_send').attr("disabled", "enabled");
                        },
                        success: function (data) {

                        },
                        error: function (data) {
                            console.log(data.responseText);
                        }
                    });
                });
            }
        });

    });

    function FindSessionNow() {

        $.ajax({
            url: '/User/FindSessionNow',
            type: 'post',
            data: {},
            beforeSend: function () {

            },
            complete: function () {

            },
            success: function (data) {
                if (data == "NoSessions") {
                    window.location.href = "/Doctor/SearchSessions";
                }
                else {
                    window.location.href = "/Doctor/Purchase/" + data.ID + "?MSG=1";
                }
            },
            error: function (data) {
                console.log(data.responseText);
            }
        });
    }

    function ShowDetailsPage(ID) {

        window.open('', '_blank').location.href = "/Home/PostDetails/" + ID;
    }

</script>
